{
  "name": "stroova",
  "version": "2.0.0",
  "private": true,
  "description": "STroova: language learning app with exercises and dictionary.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "seed": "npx tsx server/seed-dictionary.mjs",
    "seed:house": "node server/seed-house-branch.mjs",
    "update-ipa": "node server/update-ipa-from-dictionary.mjs",
    "generate-audio": "node server/generate-audio.mjs",
    "telegram-bot": "node server/telegram-bot.js",
    "build:local": "vite build --mode development",
    "android:sync": "npm run build && node scripts/remove-audio-for-android.cjs && npx cap copy",
    "android:sync:local": "npm run build:local && node scripts/remove-audio-for-android.cjs && npx cap copy",
    "android:open": "npx cap open android"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "dotenv": "^16.3.1",
    "kokoro-js": "^1.2.1",
    "pg": "^8.11.3",
    "phonemizer": "^1.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "tsx": "^4.7.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  }
}
